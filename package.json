    "spritesmith": "3.4.0",
    "style-loader": "1.2.1",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.8.2",
    "clean-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "4.5.2",
