    "@babel/preset-react": "7.10.4",
    "@svgr/webpack": "5.4.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.0.5",
    "css-loader": "4.2.2",
    "eslint": "6.5.1",
